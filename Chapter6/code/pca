import numpy as np
from sklearn.datasets import load_iris

# iris_names = load_iris()['target_names']
# iris_types = load_iris()['target']
# iris_feature = load_iris()['feature_names']
iris_data = np.array(load_iris()['data'])

# print(iris_names)
# print(iris_types)
# print(iris_feature)
# print(iris_data)
#%%
X = iris_data
Z = np.array([(x-np.mean(x))/np.std(x) for x in X])

_,sigma,VT = np.linalg.svd(Z)
print(sigma)
#%%
k = 2
PCA = VT[:k] @ Z.T
print(PCA.T)
